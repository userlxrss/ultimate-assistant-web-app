{
  "name": "ultimate-assistant-hub",
  "version": "1.0.0",
  "description": "Ultimate Assistant Hub - Integrated productivity platform",
  "main": "index.js",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:format": "prisma format",
    "db:validate": "prisma validate",
    "db:backup": "./scripts/backup.sh",
    "db:restore": "./scripts/restore.sh",
    "build:prod": "./scripts/build.sh",
    "deploy": "./scripts/deploy.sh",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:prod": "./scripts/deploy.sh production",
    "migrate": "./scripts/migrate.sh",
    "migrate:prod": "./scripts/migrate.sh production",
    "docker:build": "docker build -t ultimate-assistant-hub .",
    "docker:run": "docker run -p 3000:3000 ultimate-assistant-hub",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:stop": "docker-compose down",
    "k8s:deploy": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "k8s:logs": "kubectl logs -f deployment/ultimate-assistant-hub -n ultimate-assistant-hub",
    "health": "curl -f http://localhost:3000/api/health || echo 'Health check failed'",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "productivity",
    "assistant",
    "tasks",
    "calendar",
    "journal",
    "contacts",
    "nextjs",
    "typescript",
    "prisma"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@hookform/resolvers": "^3.9.0",
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/node": "^24.9.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.5.4",
    "lucide-react": "^0.546.0",
    "next": "^15.5.6",
    "postcss": "^8.5.6",
    "prisma": "^6.17.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.4.1",
    "recharts": "^3.3.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "uuid": "^11.1.0",
    "zod": "^3.23.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^10.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
