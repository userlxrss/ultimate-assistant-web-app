{
  "name": "productivity-hub",
  "version": "1.0.1",
  "description": "A comprehensive productivity assistant with journal, tasks, calendar, email, contacts, and analytics features",
  "private": true,
  "type": "module",
  "keywords": [
    "productivity",
    "dashboard",
    "analytics",
    "journal",
    "tasks",
    "calendar",
    "email",
    "contacts",
    "react",
    "typescript",
    "glassmorphism",
    "personal-assistant",
    "time-management",
    "organization",
    "productivity-tools"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "security-audit": "npm audit --audit-level=moderate",
    "security-fix": "npm audit fix --force",
    "gmail-server": "node gmail-imap-server.cjs",
    "start-gmail": "chmod +x start-gmail-integration.sh && ./start-gmail-integration.sh",
    "oauth-server": "node simple-oauth-server.cjs",
    "start-oauth": "npm run oauth-server",
    "install-oauth-deps": "npm install express cors googleapis express-session memorystore dotenv",
    "legacy-carddav": "node working-carddav-bridge.cjs",
    "start-legacy-carddav": "chmod +x start-working-carddav.sh && ./start-working-carddav.sh"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
