    "@supabase/supabase-js": "^2.78.0",
    "@types/express": "^5.0.3",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.12.2",
    "chart.js": "^4.4.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "firebase": "^12.5.0",
    "googleapis": "^164.1.0",
    "imap": "^0.8.19",
    "joi": "^17.9.2",
    "lucide-react": "^0.294.0",
    "mailparser": "^3.7.5",
    "memorystore": "^1.6.7",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.10",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
